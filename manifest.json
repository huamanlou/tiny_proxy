{
  "name": "迷你代理",
  "description": "tiny proxy",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "128": "img/icon.png"
  },
  "action": {
    "default_title": "tiny proxy",
    "default_icon": {
      "16": "img/icon.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["proxy", "storage", "privacy"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
